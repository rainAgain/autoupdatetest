{
  "name": "testauto",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder ./"
  },
  "build": {
    "appId":"com.electron.testauto",
    "productName": "testauto",
    "files":[
        "app/**/*",
        "main.js",
        "node_modules",
        "package.json"
    ],
    "win": {
        "target": [
            "nsis"
        ]
    },
    "nsis": {
        "oneClick": "false",
        "allowToChangeInstallationDirectory": "true"
    },
    "publish": [{
         "provider": "github",
         "owner": "rainagain",
         "repo": "autoupdatetest"
     }],
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "jjj",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.45.5"
  }
}
